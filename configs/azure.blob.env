# Azure Blob Storage (native) configuration
# DO NOT COMMIT SECRETS. Copy to configs/azure.blob.env.local and edit there.

# Either set a connection string:
# export AZURE_STORAGE_CONNECTION_STRING="DefaultEndpointsProtocol=https;AccountName=YOUR_ACCOUNT;AccountKey=...;EndpointSuffix=core.windows.net"

# Or set account + key + endpoint explicitly:
export AZURE_STORAGE_ACCOUNT=YOUR_ACCOUNT
export AZURE_STORAGE_KEY=REPLACE_ME
export AZURE_BLOB_ENDPOINT=https://YOUR_ACCOUNT.blob.core.windows.net

# Default container (aka bucket) name for this project
export AZURE_BLOB_CONTAINER=your-container

